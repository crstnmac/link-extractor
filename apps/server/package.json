{
	"name": "server",
	"main": "src/index.ts",
	"type": "module",
	"scripts": {
		"build": "tsc && tsc-alias",
		"check-types": "tsc --noEmit",
		"compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
		"dev": "bun run --hot src/index.ts",
		"start": "bun run dist/src/index.js"
	},
	"dependencies": {
		"@hono/node-server": "^1.14.1",
		"axios": "^1.9.0",
		"cheerio": "^1.0.0",
		"dotenv": "^16.4.7",
		"hono": "^4.7.6",
		"xml2js": "^0.6.2",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@types/bun": "^1.2.6",
		"@types/xml2js": "^0.4.14",
		"tsc-alias": "^1.8.11",
		"typescript": "^5.8.2"
	}
}
